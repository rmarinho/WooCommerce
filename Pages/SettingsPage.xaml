<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:local="clr-namespace:WooCommerce;assembly=WooCommerce" Title="{Binding PageName}" IsBusy="{Binding IsBusy}" BackgroundColor="{StaticResource PageBackground}" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="WooCommerce.SettingsPage">
	<ContentPage.BindingContext>
		<local:SettingsViewModel />
	</ContentPage.BindingContext>
	<ContentPage.Content>
		<Grid>
			<Button Text="Clear cache" Command="{Binding ClearCacheCommand}" />
		</Grid>
	</ContentPage.Content>
</ContentPage>